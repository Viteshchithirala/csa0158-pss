#include<stdio.h>
int main()
{
	int score;
	printf("enter score(0-100):");
	scanf("%d",&score);
	switch(score/10)
	{
		
		case 10:
			printf("grade:S");
			break;
		case 9:
			printf("grade:A");
			break;
		case 8:
			printf("grade:B");
			break;
		case 7:
			printf("grade:C");
			break;
		case 6:
			printf("grade:D");
			break;
		case 5:
			printf("grade:E");
			break;
	default:
		printf("Grade:F");
		break;
}
return 0;
}
